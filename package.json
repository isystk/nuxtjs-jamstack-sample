{
  "name": "nuxt3-jamstack-blog",
  "description": "Nuxt3・MicroCMS・GithubActionを利用した JAMStack のサンプルアプリケーションです。",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "start": "nuxi preview",
    "allupdate": "yarn upgrade-interactive --latest",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lintfix": "eslint --fix --ext .js,.ts,.vue .",
    "format": "prettier -w 'src/*.{ts,vue}' && prettier -w 'src/**/*.{ts,vue}'",
    "fix": "yarn format && yarn lint",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@intlify/nuxt3": "^0.2.2",
    "@mdi/font": "^6.7.96",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/tailwindcss": "^5.1.2",
    "@pinia/nuxt": "^0.1.9",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/vue3": "^6.5.6",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/eslint": "^8.4.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/lodash": "^4.14.182",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vuetify/vite-plugin": "^1.0.0-alpha.11",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "nuxt": "3.0.0-rc.3",
    "pinia": "^2.0.14",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "1.52.1",
    "sass-loader": "13.0.0",
    "storybook-builder-vite": "^0.1.23",
    "storybook-vue3-router": "^2.2.1",
    "stylelint": "^14.9.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.7.2",
    "vee-validate": "^4.5.11",
    "vitest": "^0.13.1",
    "vue-lodash": "^2.1.2",
    "vuejs-image-base64": "^1.2.0",
    "vuetify": "^3.0.0-beta.2",
    "yup": "^0.32.11"
  }
}
